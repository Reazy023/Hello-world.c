#include<stdio.h>
int main  () {
int i,total=0;

int revenue [7]={2,3,4,6,8,10,12};

printf ("%d", revenue[0]);
printf ("%d", revenue [1]);
printf ("%d", revenue [2]);
printf ("%d", revenue [3]);
printf ("%d", revenue [4]);
printf ("%d", revenue [5]);
printf ("%d\n", revenue [6]);
total+=revenue[i];

printf("%d", total);

return 0;
}
